{
  "coinranking": {
    "name": "Coinranking",
    "category": "crypto",
    "rapidapi_host": "coinranking1.p.rapidapi.com",
    "env_key": "RAPIDAPI_KEY",
    "keywords": ["bitcoin", "BTC", "ethereum", "ETH", "solana", "SOL", "crypto", "cryptocurrency", "token", "coin"],
    "endpoints": {
      "coin_price": {
        "method": "GET",
        "path": "/coin/{coinId}/price",
        "description": "Get current price for a coin",
        "params": {
          "coinId": { "type": "string", "required": true, "default": "Qwsogvtv82FCd" }
        }
      },
      "coins": {
        "method": "GET",
        "path": "/coins",
        "description": "List top coins with prices",
        "params": {
          "limit": { "type": "number", "required": false, "default": 10 }
        }
      }
    }
  }
}
